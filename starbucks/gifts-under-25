function renewUrl(thisObject){
var url = thisObject.attr("href").split("?");
var temppid = thisObject.attr("href").split("pid");
if(temppid.length>1){
var pid = temppid[1].split("&");
url[0] = url[0] + "?" + "pid" + pid[0] + tempPar;
}
else{
url[0] = url[0] + "?" +tempPar;
}
thisObject.attr("href",url[0]);
};
function renewUrl2(thisObject){
var url = thisObject.attr("href").split("?");
var refments2 = {};
var patt = /pref+.\d/;
var tempParray = (url.length > 1) ? url[1].split("&") : [];
for(i=0; i<tempParray.length; i++){
var tempPars = tempParray[i].split("=");
refments2[tempPars[0]] = tempPars[1];
}
for(i in refments){
if(i=="listview" || (refments[i]!=null && i!="format" && !patt.test(i))){
refments2[i] = refments[i];
}
}
url[1]="";
for(i in refments2){
if(refments2[i]!=null && i!="format"){
console.log(i+"="+refments2[i]);
url[1] = url[1] + "&" + i + "=" + refments2[i];
}
}
url[0] = url[0] + "?" + url[1];
thisObject.attr("href",url[0]);
};
jQuery(".productresultarea .productimage a, .productresultarea .lvname a, .productresultarea a.overlay, .productresultarea .name a").click(function(e){
e.preventDefault();
renewUrl(jQuery(this));
window.location.href = jQuery(this).attr("href");
});
jQuery("#searchrefinements .refineattributes li a, #searchrefinements .refinedclear a").click(function(e){
e.preventDefault();
renewUrl2(jQuery(this));
window.location.href = jQuery(this).attr("href");
});
if($(window).width()<=1024){
app.getReadMore();
}
else{}
});
