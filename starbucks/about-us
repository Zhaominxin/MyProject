	<iframe name="__bkframe" height="0" width="0" frameborder="0" src="javascript:void(0)"></iframe>
	<script type="text/javascript" src="https://tags.bkrtx.com/js/bk-coretag.js"></script>
			
<script type="text/javascript">
	/* <![CDATA[ */
	jQuery(document).ready(function() {
		var page = "null";
		var cgid = "null";
		var qValue = "";
		var cid = "";

		app.formvalidation.init(null,"returningcustomers","btn_login");
		
		if (jQuery("#PasswordResetDialog").length <=0) {
			// create this div only once
			jQuery("<div/>").attr("id", "PasswordResetDialog").appendTo(document.body);
		}

		var form = jQuery("#dwfrm_login");
		var username = jQuery("input[name=dwfrm_login_username]");
		var usernameInfo = jQuery(".errormessageH");
		var password = jQuery("input[name=dwfrm_login_password]");
		var passwordInfo = jQuery(".errormessageH");

		var errmsg = jQuery(".errormessageH");

		//On Submit
		form.submit(function(){
			pfs_cmCreateRegistrationTag(jQuery("[id=dwfrm_login_username]").val());
			var responsysRetargeterEnabled = 'true';
			if(responsysRetargeterEnabled == 'true'){
				bkCoreTag();
			}
			return !!CheckLogin();
		});
		
		function bkCoreTag() {
			var responsysSiteId = '27227';
			var e_id_m = 'e_id_m'+responsysSiteId;
			var e_id_s = 'e_id_s'+responsysSiteId;
			var email = $("#dwfrm_login_username").val();
			BKTAG.util.normalizeEmail(email);
			BKTAG.addHash(e_id_m, e_id_s, email);
			bk_doJSTag(responsysSiteId, 1);
		}
		
		function CheckLogin(){
			var $form = $('#dwfrm_login'),
				username = $form.find("input[name=dwfrm_login_username]"),
				password = $form.find("input[name=dwfrm_login_password]"),
				$csrftoken_elem = $form.find('#csrf-token'),
				csrftoken_name = $csrftoken_elem.attr('name'),
				post = "username="  + username.val() + "&password=" + password.val() + '&'+csrftoken_name+'=' + $csrftoken_elem.val(),
				notFound = "NOT FOUND",
				locked	= "LOCKED";

		 	jQuery.ajax({
				type: "POST",
				url: app.URLs.checkLogin,
				dataType:"html",
				data: post,
				success: function (data, textStatus, xhr) {
					var json = null;
					if (data == notFound ) {
						errmsg.html("The email address and/or password could not be found.Please enter a valid email address and password combination.").fadeIn();
						return false;
					} else if(data == locked ){
						errmsg.html("Oops! Your account has been locked. Please call to Customer Care to Unlock the account.").fadeIn();
						return false;
					} else if (xhr.getResponseHeader('Content-Type') === 'application/json') {
						json = JSON.parse(data);
						
						if ('success' in json && !json.success) {
							errmsg.html(json.message).fadeIn();
							return false;
						}
					}
					
					switch(page){
					 
					   case 'Home'              : document.location.href = "https://www.teavana.com/us/en/home?ishopup=true";
													return true;
													break;
					   case 'Search'   	    	: 	if (cgid.length > 0 && qValue <= 0) {
														document.location.href = "https://www.teavana.com/us/en/search?cgid=null&amp;ishopup=true";
													} else if (qValue.length > 0 && cgid.length <= 0){
														document.location.href = "https://www.teavana.com/us/en/search?ishopup=true";
													}
													return true;
													break;
					   case 'PDP'               : document.location.href = "https://www.teavana.com/on/demandware.store/Sites-Teavana-Site/default/Product-Show?pid=&amp;ishopup=true";
													return true;
													break;
										
					   case 'SPC'        		: document.location.href = "https://www.teavana.com/us/en/cart?ishopup=true";
													return true;
													break;				
					   case 'PageShow'   		: 	if (cid.length > 0){
														document.location.href = "https://www.teavana.com/on/demandware.store/Sites-Teavana-Site/default/Page-Show?cid=&amp;ishopup=true";
													} else {
														document.location.href = "https://www.teavana.com/us/en/sitemap?ishopup=true";
													}
													return true;
													break;				
					   case 'OrderConfirmation' : document.location.href = "https://www.teavana.com/on/demandware.store/Sites-Teavana-Site/default/COSummary-Submit?ishopup=true";
													return true;
													break;
						default 				: 	window.location.reload();	
													return true;						   		
					}
				}
			});
		}
		
		jQuery("[id=dwfrm_login_password]").keydown(function (e) {
			if (e.keyCode == 13) {
				e.preventDefault();
				form.submit();
			}
		});

		jQuery("[id=dwfrm_requestpassword_email]").keydown(function (e) {
			if (e.keyCode == 13) {
				e.preventDefault();
				jQuery("[id=dwfrm_requestpassword_confirm]").click();
			}
		});

		jQuery("a.createaccountheaderLabel").click(function(){
			cmCreateConversionEventTag('Create an Account', 1, 'My Account', 0, null);
		});
	});
	
	/* ]]> */
</script>

</div>

</div>


