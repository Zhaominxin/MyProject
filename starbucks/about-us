<script>
	var _a1as = _a1as || [];
	jQuery(document).ready(function() {
		jQuery(".simplesearchinput").focus(function() {
			this.value = "";
		});
	
		jQuery(".simplesearch #btnSubmit").click(function() {
			if (jQuery.trim(jQuery("#searchinput").val())=="" || jQuery.trim(jQuery("#searchinput").val().toLowerCase())=="search" || jQuery.trim(jQuery("#searchinput").val().toLowerCase().replace("#",""))=="enter keyword or item") {
				return false;
			} else if (app.prefs.agilOneWebTagEnabled) {
				_a1as.push(["clear"]);
				_a1as.push(["setvar","onsitesearch",jQuery("#searchinput").val()]);			
				_a1as.push(["track"]);
				return true;
			}
		});
	});
</script>
				
