<script language="javascript" type="text/javascript">
var tempPar="";
if(refments == null){
var refments = {};
refments.srule = "Featured";
refments.start = 0.0;
refments.sz = 12;
if(window.location.hash.length>0){
tempVar = window.location.hash.substring(1, window.location.hash.length).match(/([^\&]+)=([^\&]+)/g);
for(var i=0; i<tempVar.length; i++){
var temp1 = tempVar[i].split("=");
refments[temp1[0]] = temp1[1];
}
}
jQuery(".httpParams").each(function(){
refments[jQuery(this).attr("parameterName")]=jQuery(this).attr("parameterValue");
});
for(i in refments){
if(refments[i]!=null && i!="format") tempPar += "&"+i+"="+refments[i];
}
}
else{
jQuery(".httpParams").each(function(){
refments[jQuery(this).attr("parameterName")]=jQuery(this).attr("parameterValue");
});
for(i in refments){
if(refments[i]!=null && i!="format") tempPar += "&"+i+"="+refments[i];
}
window.location.hash=tempPar;
}
function reloadRefinement(){
var url = "http://www.teavana.com/us/en/search";
var thisParams = "";
for(i in refments){
if(refments[i]!=null && i!="format"){
thisParams += "&"+i+"="+refments[i];
}
}
var newUrl = url+"?"+thisParams;
newUrl = encodeURIComponent(newUrl).replace(/%23/g,"");
newUrl = decodeURIComponent(newUrl);
newUrl = newUrl.replace(/\s/g,"%20");
console.log(newUrl);
app.search.result.updateGrid(newUrl);
}
function reserveEmailSignup(options) {
app.reserveListDialog(options);
return false;
}
function ccAddtowishlist(attr){
cmCreateConversionEventTag("Category:Add to My Wishlist","2","Wishlist","0",removeQuotesAndR(attr));
//BB-199 : Teavana_SBX_SecurityIssue_June
var pidArray=attr.split('-_-');
var pid=pidArray[0];
var params = {'pid':pid,'format':'postredirect'};
var url = app.URLs.wishlistadd;
app.util.postObject(url, params, true);
}
jQuery(document).ready(function() {
// Check first if the list is in the grid view
var listview = "";
