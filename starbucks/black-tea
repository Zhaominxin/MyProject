		<script>														
			jQuery(".pagination a").bind("click",function(){							
				if(jQuery(".pt_productsearchresult").length>0){										
					var url = jQuery(this).attr("href").replace(/&format=ajax/g, "");
					var hash = url.split("?");
					window.location.hash = "?" + hash[1];
				}	
			});							
		</script>
	

</noscript>
</td>
</tr>
</table><!-- END: controlsright -->
