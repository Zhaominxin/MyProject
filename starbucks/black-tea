<script>
jQuery(document).ready(function() {
	jQuery('.starvalue_link').hover(function(){
		
		var top_offset = 0;
		var left_offset = 0;
		
		//var top_offset = jQuery("#content").offset().top;
		//var left_offset = jQuery("#content").offset().left;
		
		
		var thisTop = ( jQuery(window).height() - jQuery("#starvalue_info").height() ) / 2 ;
		var thisLeft = ( jQuery(window).width() - jQuery("#starvalue_info").width() ) / 2;
		
		var pTop = thisTop - top_offset;
		var pLeft = thisLeft - left_offset;
		

		jQuery("#starvalue_info").css({
			'z-index': '9999',
			'position': 'absolute',
			'top': + pTop + 'px',
			'left': + pLeft + 'px',
                       'display': 'block'
                      

		
	}); 
     }, function(){
                        jQuery("#starvalue_info").css('display','none'); 

				
});		
	jQuery('.closeicon').click(function(){		
		jQuery("#starvalue_info").fadeToggle();
	});
	
 
});
</script>
