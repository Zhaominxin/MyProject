<script language="javascript" type="text/javascript">
var tempPar="";
if(refments == null){
var refments = {};
refments.srule = "Featured";
refments.start = 0.0;
refments.sz = 12;
if(window.location.hash.length>0){
tempVar = window.location.hash.substring(1, window.location.hash.length).match(/([^\&]+)=([^\&]+)/g);
for(var i=0; i<tempVar.length; i++){
var temp1 = tempVar[i].split("=");
refments[temp1[0]] = temp1[1];
}
}
jQuery(".httpParams").each(function(){
refments[jQuery(this).attr("parameterName")]=jQuery(this).attr("parameterValue");
});
for(i in refments){
if(refments[i]!=null && i!="format") tempPar += "&"+i+"="+refments[i];
}
}
else{
jQuery(".httpParams").each(function(){
refments[jQuery(this).attr("parameterName")]=jQuery(this).attr("parameterValue");
});
for(i in refments){
if(refments[i]!=null && i!="format") tempPar += "&"+i+"="+refments[i];
}
window.location.hash=tempPar;
}
function reloadRefinement(){
var url = "http://www.teavana.com/us/en/search";
var thisParams = "";
for(i in refments){
if(refments[i]!=null && i!="format"){
thisParams += "&"+i+"="+refments[i];
}
}
var newUrl = url+"?"+thisParams;
newUrl = encodeURIComponent(newUrl).replace(/%23/g,"");
newUrl = decodeURIComponent(newUrl);
newUrl = newUrl.replace(/\s/g,"%20");
console.log(newUrl);
app.search.result.updateGrid(newUrl);
}
function reserveEmailSignup(options) {
app.reserveListDialog(options);
return false;
}
function ccAddtowishlist(attr){
cmCreateConversionEventTag("Category:Add to My Wishlist","2","Wishlist","0",removeQuotesAndR(attr));
//BB-199 : Teavana_SBX_SecurityIssue_June
var pidArray=attr.split('-_-');
var pid=pidArray[0];
var params = {'pid':pid,'format':'postredirect'};
var url = app.URLs.wishlistadd;
app.util.postObject(url, params, true);
}
jQuery(document).ready(function() {
// Check first if the list is in the grid view
var listview = "";
jQuery("#mspage1").addClass("selected");
jQuery("#mspage2, #mspage3, #mspage4").click(function(){
jQuery("#mspage1").css("text-decoration","underline");
});
jQuery("#mspage1").click(function(){
jQuery("#mspage1").css("text-decoration","none");
});
jQuery(".resultshitShowAll").click(function(e){
e.preventDefault();
refments.showall=true;
refments.start = null;
refments.sz = null;
reloadRefinement();
return false;
});
jQuery("input.gridstar").rating();
jQuery("#sortrule").change(function(){
refments.srule = jQuery("#sortrule option:selected").attr("sort").replace(/\s/g,"%20");
refments.start = 0;
reloadRefinement();
});
jQuery("#search div.pagination a").unbind();
jQuery("#search div.pagination a").click(function(e) {
e.preventDefault();
if(jQuery(this).parents("li").hasClass("unselectable")) return false;
refments.start = jQuery(this).attr("href").match(/start=([^\&]+)/g)[0].replace(/start=/g,"");
reloadRefinement();
});
function renewUrl(thisObject){
var url = thisObject.attr("href").split("?");
var temppid = thisObject.attr("href").split("pid");
if(temppid.length>1){
var pid = temppid[1].split("&");
url[0] = url[0] + "?" + "pid" + pid[0] + tempPar;
}
else{
url[0] = url[0] + "?" +tempPar;
}
thisObject.attr("href",url[0]);
};
function renewUrl2(thisObject){
var url = thisObject.attr("href").split("?");
var refments2 = {};
var patt = /pref+.\d/;
var tempParray = (url.length > 1) ? url[1].split("&") : [];
for(i=0; i<tempParray.length; i++){
var tempPars = tempParray[i].split("=");
refments2[tempPars[0]] = tempPars[1];
}
for(i in refments){
if(i=="listview" || (refments[i]!=null && i!="format" && !patt.test(i))){
refments2[i] = refments[i];
}
}
url[1]="";
for(i in refments2){
if(refments2[i]!=null && i!="format"){
console.log(i+"="+refments2[i]);
url[1] = url[1] + "&" + i + "=" + refments2[i];
}
}
url[0] = url[0] + "?" + url[1];
thisObject.attr("href",url[0]);
};
jQuery(".productresultarea .productimage a, .productresultarea .lvname a, .productresultarea a.overlay, .productresultarea .name a").click(function(e){
e.preventDefault();
renewUrl(jQuery(this));
window.location.href = jQuery(this).attr("href");
});
jQuery("#searchrefinements .refineattributes li a, #searchrefinements .refinedclear a").click(function(e){
e.preventDefault();
renewUrl2(jQuery(this));
window.location.href = jQuery(this).attr("href");
});
if($(window).width()<=1024){
app.getReadMore();
}
else{}
});
</script>
