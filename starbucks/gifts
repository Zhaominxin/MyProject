}
//fallback svg image for ie8 , Android 2.3 and down.
jQuery(document).ready(function(){
if (!Browser.Can.Placeholder()) {
var myplaceholder = "SEARCH";
jQuery(".simplesearchinput#searchinput").val(myplaceholder);
jQuery(".simplesearchinput#searchinput").click(function(){
//if(jQuery("#searchinput.simplesearchinput").val() == myplaceholder){
jQuery(".simplesearchinput#searchinput").val('');
//}
});
$("#searchinput.simplesearchinput").blur(function(){
if(jQuery(".simplesearchinput#searchinput").val() ==''){
jQuery(".simplesearchinput#searchinput").val(myplaceholder).removeClass('fb_placeholder').addClass('fb_placeholder');
}
})
}
//TODO: remove this block, and handle with pixelratio-based media query
if( !('backgroundSize' in document.documentElement.style)){
jQuery("#countryselect .flag , #selectlocale .flag").removeClass('ie8fallback').addClass("ie8fallback");
jQuery("#countryselect .us.flag , #selectlocale .us.flag").removeClass('ie8fallback').addClass("ie8fallback");
jQuery("#countryselect .uk.flag , #selectlocale .uk.flag").removeClass('ie8fallback').addClass("ie8fallback");
jQuery("#countryselect .fr.flag , #selectlocale .fr.flag").removeClass('ie8fallback').addClass("ie8fallback");
jQuery("#countryselect .ca.flag , #selectlocale .ca.flag").removeClass('ie8fallback').addClass("ie8fallback");
jQuery("#countryselect .country .flag").removeClass("ie8fallback").addClass("ie8fallback");
jQuery(".footercontent .social .icons a").removeClass("ie8fallback").addClass("ie8fallback");
jQuery(".footercontent .logo").removeClass("ie8fallback").addClass("ie8fallback");
}
if ( ('errorpage' in window && window.errorpage) || ('Checkout' in window && window.Checkout) || ('confirmation' in window && window.confirmation) ) {
$("#searchinput").css({"display":"none", "visibility":""});
$("#btnSubmit").css({"display":"none", "visibility":""});
$(".headercustomerinfo").css({"display":"none", "visibility":""});
$('.categorymenu li').css({display: 'none'});
$(".headerbannerslotcontainer").css({"display":"none", "visibility":""});
$('#logo').css('top' , '10px');
$('#logo').children().css('top' , '25px');
$('#minicart').css({top: '25px'});
}
});
</script>





