<script type="text/javascript">
function resetMiniCartTabs() {
if (jQuery("#selBag").is(".selected")) {
jQuery("#selBag").css("background-color","white");
jQuery("#selWl").css("background-color","transparent");
jQuery("#selBag .roundednav").show();
jQuery("#selWl .roundednav").hide();
}
else if (jQuery("#selWl").is(".selected")) {
jQuery("#selWl").css("background-color","white");
jQuery("#selBag").css("background-color","transparent");
jQuery("#selWl .roundednav").show();
jQuery("#selBag .roundednav").hide();
}
}
jQuery(document).ready(function(){ // wait for document to load
jQuery("#mbpageleft").click(
function(){
app.minicart.scroll("left");
});
jQuery("#mbpageright").click(
function(){
app.minicart.scroll("right");
});
});
</script>
